<!DOCTYPE html>
<html lang="ar">
<head>
  <meta charset="UTF-8" />
  <title>بيانات المستخدمين</title>
</head>
<body>
  <h1>قائمة المستخدمين</h1>
  <pre id="data">جاري تحميل البيانات...</pre>

  <script>
    async function fetchData() {
      try {
        const response = await fetch('https://raw.githubusercontent.com/USERNAME/REPO/main/users_data.json');
        const data = await response.json();
        document.getElementById('data').textContent = JSON.stringify(data, null, 2);
      } catch (e) {
        document.getElementById('data').textContent = 'فشل تحميل البيانات!';
      }
    }
    fetchData();
  </script>
</body>
</html>
